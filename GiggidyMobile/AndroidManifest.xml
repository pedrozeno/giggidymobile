<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectattempt"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="ncirl.project.giggidymobileapp.utils.AppController"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ncirl.project.giggidymobileapp.facebook.FacebookLoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FacebookLoginActivity"
            android:label="@string/title_activity_logon" >
        </activity>
        <activity
            android:name="ncirl.project.giggidymobileapp.parse.SignUpActivity"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="ncirl.project.giggidymobile.lastfm.GigDetailActivity"
            android:label="@string/title_activity_gig_list" >
        </activity>
        <activity
            android:name="ncirl.project.giggidymobile.lastfm.GigListActivity"
            android:label="@string/title_activity_gig_list" >
        </activity>
        <activity
            android:name="com.facebook.LoginActivity"
            android:label="@string/app_name" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity
            android:name="ncirl.project.giggidymobileapp.facebook.UserProfileActivity"
            android:label="@string/title_activity_user_account" >
        </activity>
        <activity
            android:name="ncirl.project.giggidymobileapp.facebook.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="ncirl.project.giggidymobileapp.facebook.PlannerActivity"
            android:label="@string/title_activity_planner" >
        </activity>
    </application>

</manifest>
